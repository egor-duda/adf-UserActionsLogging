<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <action-listener>corpit.test.audit.jsf.GlobalActionListener</action-listener>
    <system-event-listener>
      <system-event-listener-class>corpit.test.audit.jsf.GlobalListenerInjector</system-event-listener-class>
      <system-event-class>javax.faces.event.PreRenderViewEvent</system-event-class>
    </system-event-listener>
  </application>
  <lifecycle>
    <phase-listener>corpit.test.audit.jsf.ReplayUserInputPhaseListener</phase-listener>
  </lifecycle>
</faces-config>